		if key.KeyCode == Enum.KeyCode[keyh] then
			if toggle then
				toggle = false
				Main:TweenSize(UDim2.new(0, 740, 0, 0),"Out","Quint",0.3,true,nil)
				wait(.3)
				topb.Visible = false
			else if not toggle then
					toggle = true
					topb.Visible = true
					Main:TweenSize(UDim2.new(0, 740, 0, 459),"In","Quint",0.3,true,nil)
				end
